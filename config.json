export const debug=process.env.DEBUG?.toLowerCase()=='true'
export const token=process.env.DISCORD_BOT_TOKEN
export const clientId=process.env.DISCORD_CLIENT_ID
export const leaveOnEnd=process.env.BOT_LEAVE_ON_QUEUE_END?.toLowerCase()!='false'
export const leaveOnEndTimeout=(() => {
  let timeout=0
  try {
    timeout=parseInt(process.env.BOT_LEAVE_ON_QUEUE_END_TIMEOUT)
  }
catch {
timeout=0
}
return timeout
})()
export const leaveOnEmpty=process.env.BOT_LEAVE_ON_EMPTY_CHANNEL?.toLowerCase()!='false'
export const leaveOnEmptyTimeout=(() => {
let timeout=0
try {
timeout=parseInt(process.env.BOT_LEAVE_ON_EMPTY_CHANNEL_TIMEOUT)
}
catch {
timeout=0
}
return timeout
})()
export const initialVolume=(() => {
let initialVolume=100
try {
initialVolume=parseInt(process.env.BOT_INITIAL_VOLUME)
}
catch(error) {
initialVolume=100
}
return initialVolume
})()
